package com.study.tune.entity;

public enum TuneEnum {
	ELECTRONIC(new ElectronicTune()),
	INSTRUMENTAL(new InstrumentalTune()),
	VOCAL(new VocalTune()); 
	
	private Tune tune;
	
	private TuneEnum(Tune tune){
		this.setTune(tune);
	}

	public Tune getTune() {
		return tune;
	}

	public void setTune(Tune tune) {
		this.tune = tune;
	}
}
