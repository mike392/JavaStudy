package com.study.tune.action;

import java.util.List;

import com.study.tune.creator.TuneCreator;
import com.study.tune.entity.Tune;
import com.study.tune.util.Constants;

public class TuneListPopulator {
	public static void populateTuneListGivenInput(List<Tune> tuneList, List<String> input){
		for (String item : input){
			String tuneType = item.split(Constants.COLUMN_DELIMITER)[0];
			Tune tune = (Tune) TuneCreator.createTuneBasedOnType(tuneType);
			TuneList.addTune(tune);
		}
	}
}
