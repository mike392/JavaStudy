package com.study.abiturient.abiturientlist.utils;

import java.util.HashMap;
import java.util.LinkedList;
import java.util.Map.Entry;
import java.util.SortedSet;
import java.util.TreeSet;

import com.study.abiturient.abiturientlist.abiturient.Abiturient;

public class AbiturientListGenerator {
	
	public static SortedSet<Abiturient> generateAbiturientList(HashMap<String, String> listWithMarks){
		SortedSet<Abiturient> result = new TreeSet<Abiturient>();
		if (listWithMarks.size() > 0){
			for (Entry entry : listWithMarks.entrySet()){
				Abiturient abiturient = new Abiturient();
				String abiturientName = MarksListValidator.validateString(entry.getValue());
				abiturient.setName(entry.getKey());
			}
		}
		return result;
	}
}
